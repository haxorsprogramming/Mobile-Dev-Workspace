<script>
	function buatPesan()
	{
	document.getElementById('pertama').style.background = 'green'
	document.getElementById('pertama').style.color ='blue'
	document.getElementById('pertama').style.fontsize ="50px"
	document.getElementById('kedua').style.background = 'pink'
	document.getElementByid('kedua').style.color ='yellow'
	document.getElementById('kedua').style.fontsize ="65px"
	}
	function jadiPesan()
	{
	 dpcument.getElementById('pertama').style.color = 'blue'
	}
